<!-- COCKCTAILS INDEX -->
<div class="container">
  <div class="row">
    <% @cocktails.each do |cocktail| %>
      <div class="card col-4">
        <div class="card-category" style="background-image: url(<%= cl_image_path cocktail.photo.key %>)">
          <p><%= link_to cocktail.name, cocktail_path(cocktail) %></p>
        </div>
      </div>
    <% end %>
  </div>
</div>